	@Override
	public void displayDrink(DrinkDTO[] drinkDTOs) {
		
		for (int i = 0; i < drinkDTOs.length; i++) {
			System.out.print((i+1) + "." + drinkDTOs[i].name  + "(" + drinkDTOs[i].price + ")");
			System.out.println();
		}
		
		
	}

