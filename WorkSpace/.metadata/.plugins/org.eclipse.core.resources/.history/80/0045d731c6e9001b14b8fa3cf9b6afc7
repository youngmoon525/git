package pack08_ChangeTP2;

public class ChangeMain {
	public static void main(String[] args) {
		//Child 는 Parent + Child 
		Child ch = new Child();
		ch.method1();
		ch.method2();
		ch.method3();
		ch.method4();
		//상속의 일반적인 사용방법
		
		Prent prent = ch; //
		prent.method1();
		prent.method2();
		
		//file Stream 
		//더 큰 범위의 부모 클래스가 자식클래스로 변환되서 사용되는 경우가 많이 생김
		//다형성 
	}
}
